

<div class= "container">
<%@all_users.each do |user|%>
  <div class="row">
    <div class="col s12 m4">
      <div class="card grey darken-2">
        <div class="card-content white-text">
          <span class="card-title"><center><b><%=user.name%></b></center></span>


          <%user.bags.each do |bag|%>
          <p><%=bag.name%></p>
          <%end%>
     
        </div>
        <div class="card-action">
          <center><a href="/golfers/<%=user.id%>">Show</a></center>
        </div>
      </div>
    </div>
  <%end%>
  </div>
</div>
